.detail_process .tip_bar{font-size:0.55rem;width:100%;height:0;overflow:visible}
.detail_process .tip_bar .tip{top: 1rem;position:absolute;margin-left:-1.2rem; width:2.4rem; text-align:center; border-radius: 0.2rem;height:0.9rem;line-height:0.9rem;background:#004ca2;color: #fff;font-family: Arial}
.detail_process .tip_bar span{
    width:0; 
    height:0; 
    border-left:0.3rem solid transparent;
    border-right:0.3rem solid transparent;
    border-top:0.3rem solid #004ca2;
    bottom:-0.3em;
    display:block;
    margin: 0 auto
}

