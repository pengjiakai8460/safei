{include file="./inc/header.html"}	
<div class="page" id='{$data.act}'>
{include file="./inc/title.html"}
<div class="content">
<!-- 这里是页面内容区 -->

<!--账户日志-->
<div class="uc_money_log">
	<ul>
		{foreach from=$data.item item="item"}
		<li class="uc_money_log_list ">
			<div class="clearfix">
					<div class="f_l type">
						<h4>{$item.log_info}</h4>
						<p>
							{$item.log_time_format} 
						</p>
					</div>
					<div class="f_r money">
						<div class="f_r freeze">
							<div class="freeze_son">
							<span>{$item.lock_money}</span>
							<p>冻结/提现</p>
							</div>
						</div>
						<div class="f_r  balance">
							<div class="balance_son">
							<span class="specialfont">{$item.money}</span>
							<p>金额</p>
							</div>
						</div>
						
					</div>	
			</div>
			</li>
		{/foreach} 
	</ul>
</div>

<div class="w_b but_box_parent padding">
		<div class="w_b_f_1 but_box">		    
			<a  href="#" onclick="RouterURL('{wap_url a="index" r="uc_incharge" p="id=$deal.id"}','#uc_incharge',2);" class="but_this">充值</a>
		</div>
		<div class="w_b_f_1 but_box">
			<a href="#" onclick="RouterURL('{wap_url a="index" r="uc_bank" p="id=$deal.id"}','#uc_bank',2);"  class="but_this bg_ea544a">提现</a>
		</div>
</div>
{include file="./inc/footer.html"}







